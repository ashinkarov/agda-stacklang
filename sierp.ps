45 120 translate

/PsExample.Sierpinski.draw-circ-xy {
    .3 0 360 arc fill
} def

/PsExample.Sierpinski.bit-and  {
    and
} def

/PsExample.Sierpinski.draw-if {
  0 index 0 eq 
  {
    pop 1 index 1 index PsExample.Sierpinski.draw-circ-xy
  }
  {
    pop
  } ifelse

} def


/PsExample.Sierpinski.outer-for-loop {
  0 2 index 
  1 exch
  {
  1 index 1 index PsExample.Sierpinski.bit-and PsExample.Sierpinski.draw-if pop
  
} for
 pop
} def


/PsExample.Sierpinski.sierp {
  0 1 index 
  1 exch
  {
  PsExample.Sierpinski.outer-for-loop
  
} for
 pop
} def

